{
    "private": true,
    "scripts": {
        "build": "nuxt build",
        "dev": "nuxt dev",
        "generate": "nuxt generate",
        "preview": "nuxt preview",
        "postinstall": "nuxt prepare",
        "lint": "eslint --ext .js,.ts,.vue --ignore-path .eslintignore . --ignore-pattern node_modules/",
        "lintfix": "eslint --ext --fix .js,.ts,.vue --ignore-path .eslintignore . --ignore-pattern node_modules/",
        "test": "npm-run-all postinstall && vitest --run --reporter verbose --globals",
        "test:watch": "npm-run-all postinstall && vitest --reporter verbose --globals",
        "test:ui": "npm-run-all postinstall && vitest --ui --reporter verbose --globals",
        "analyze": "nuxt build --analyze",
        "doc": "typedoc --options typedoc.json",
        "doc:components": "vuedoc-md components/*.vue --output compdocs/",
        "story:dev": "histoire dev",
        "story:build": "histoire build",
        "story:preview": "histoire preview",
        "testtest": "vitest"
    },
